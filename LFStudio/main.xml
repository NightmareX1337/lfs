<?xml version="1.0" encoding="utf-8"?>
<adce xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <For_autocompletion_window>
    <Frame>
      <init_tag>&lt;bmp_begin&gt;</init_tag>
      <close_tag>&lt;bmp_end&gt;</close_tag>
      <content_in_init_tag>false</content_in_init_tag>
      <body>
        <ArrayOfString>
          <string>name: </string>
        </ArrayOfString>
        <ArrayOfString>
          <string>head: </string>
        </ArrayOfString>
        <ArrayOfString>
          <string>small: </string>
        </ArrayOfString>
        <ArrayOfString>
          <string>file(-): </string>
          <string>w: </string>
          <string>h: </string>
          <string>row: </string>
          <string>col: </string>
        </ArrayOfString>
      </body>
      <subframes />
    </Frame>
    <Frame>
      <init_tag>&lt;frame&gt;</init_tag>
      <close_tag>&lt;frame_end&gt;</close_tag>
      <content_in_init_tag>true</content_in_init_tag>
      <body>
        <ArrayOfString>
          <string>pic: </string>
          <string>state: </string>
          <string>wait: </string>
          <string>next: </string>
        </ArrayOfString>
      </body>
      <subframes>
        <Frame>
          <init_tag>bpoint:</init_tag>
          <close_tag>bpoint_end:</close_tag>
          <content_in_init_tag>false</content_in_init_tag>
          <body>
            <ArrayOfString>
              <string>x: </string>
              <string>y:</string>
            </ArrayOfString>
          </body>
          <subframes />
        </Frame>
        <Frame>
          <init_tag>wpoint:</init_tag>
          <close_tag>wpoint_end:</close_tag>
          <content_in_init_tag>false</content_in_init_tag>
          <body>
            <ArrayOfString>
              <string>kind: </string>
              <string>x: </string>
              <string>y:</string>
              <string>weaponact: </string>
              <string>attacking: </string>
              <string>cover: </string>
              <string>dvx: </string>
              <string>dvy: </string>
              <string>dvz: </string>
            </ArrayOfString>
          </body>
          <subframes />
        </Frame>
        <Frame>
          <init_tag>bdy:</init_tag>
          <close_tag>bdy_end:</close_tag>
          <content_in_init_tag>false</content_in_init_tag>
          <body>
            <ArrayOfString>
              <string>kind: </string>
              <string>x: </string>
              <string>y:</string>
              <string>w: </string>
              <string>h: </string>
            </ArrayOfString>
          </body>
          <subframes />
        </Frame>
      </subframes>
    </Frame>
    <Frame>
      <init_tag>&lt;weapon_strength_list&gt;</init_tag>
      <close_tag>&lt;weapon_strength_list_end&gt;</close_tag>
      <content_in_init_tag>false</content_in_init_tag>
      <body />
      <subframes>
        <Frame>
          <init_tag>entry: </init_tag>
          <close_tag />
          <content_in_init_tag>true</content_in_init_tag>
          <body>
            <ArrayOfString>
              <string>dvx: </string>
              <string>fall: </string>
            </ArrayOfString>
          </body>
          <subframes />
        </Frame>
      </subframes>
    </Frame>
  </For_autocompletion_window>
