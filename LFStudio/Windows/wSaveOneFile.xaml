<Window x:Uid="Window_1" x:Class="LFStudio.wSaveOneFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Background="{DynamicResource winbkgcolor}"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        Closed="Window_Closed"
        Title="Question" Height="116" Width="353">
    <Grid x:Uid="Grid_1">
        <Button x:Uid="bSC" Content="_Save and Close" Height="23" HorizontalAlignment="Left" Margin="12,38,0,0" Name="bSC" VerticalAlignment="Top" Width="91" Click="bSC_Click" />
        <Button x:Uid="bDSC" Content="_Don't save and Close" Height="23" HorizontalAlignment="Left" Margin="109,38,0,0" Name="bDSC" VerticalAlignment="Top" Width="123" Click="bDSC_Click" />
        <Button x:Uid="bCancel" Content="_Cancel" Height="23" HorizontalAlignment="Left" Margin="238,38,0,0" Name="bCancel" VerticalAlignment="Top" Width="75" Click="bCancel_Click" />
        <Label x:Uid="lMessage" Content="File modified and don't saved. What do you want to do?" Height="28" HorizontalAlignment="Left" Margin="12,7,0,0" Name="lMessage" VerticalAlignment="Top" Width="313" />
    </Grid>
</Window>
