<Window x:Uid="Window_1" x:Class="LFStudio.wCreateNewFolder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="wCreateNewFolder" Height="136" Width="444">
    <Grid x:Uid="Grid_1">
        <Label x:Uid="l" x:Name="l" Content="Type name for new folder"></Label>
        <Button x:Uid="button1" Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,62,93,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button x:Uid="button2" Content="Chancel" Height="23" HorizontalAlignment="Left" Margin="335,62,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        <TextBox x:Uid="tbFolderName" Height="23" HorizontalAlignment="Left" Margin="12,27,0,0" Name="tbFolderName" VerticalAlignment="Top" Width="398" />
    </Grid>
</Window>
